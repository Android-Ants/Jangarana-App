<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".fragments.familyDetail9Fragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="45dp"
            android:fontFamily="@font/dm_mono"
            android:text="CENSUS  FORM"
            android:textColor="@color/color_primary"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="16dp"
            android:gravity="center_horizontal"
            android:text="Fill your Household details here"
            android:textColor="#99000000"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="50dp"
            android:fontFamily="@font/dm_mono"
            android:text="Check all the boxes for the things the household has"
            android:textColor="@color/color_primary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView10" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="22dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="24dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView17">

            <CheckBox
                android:id="@+id/isRadio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Radio/Transistor"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />

            <CheckBox
                android:id="@+id/isTelivision"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Telivision"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />

            <CheckBox
                android:id="@+id/isInternet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Access to the internet"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />

            <CheckBox
                android:id="@+id/isLaptop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Laptop/Computer"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />

            <CheckBox
                android:id="@+id/isPhone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Telephone/Mobile Phone/Smartphone"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />

            <CheckBox
                android:id="@+id/isBicycle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Bicycle/Scooter/Moped/Motorcycle"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />

            <CheckBox
                android:id="@+id/isCar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Car/Jeep/Van"
                android:textColor="@color/color_primary"
                android:textSize="16sp" />
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/generalHouseholdClearall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/ic_clear_all_button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/generalHouseholdNext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="40dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/ic_next_button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>