<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.woc.jangarana">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jangarana">
        <activity
            android:name=".authentication.LoadingScreenActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".authentication.staff.StaffLoginActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".staff.StaffDashboardActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".familyhead.DashboardActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".authentication.familyhead.VerifyEmailActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".authentication.LoginOptionsActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".authentication.familyhead.FamilyHeadSignUpActivity"
            android:theme="@style/Theme.Jangarana"
            android:exported="false" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.Jangarana"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>